<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections />
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
	</startup>
	<connectionStrings>
		<clear />
		<add name="MsSqlSourceConnection" connectionString="Data Source=localhost;Initial Catalog=SourceDatabase;User ID=username;password=password;Integrated Security=False;" providerName="System.Data.SqlClient" />
		<add name="MsSqlDestinationConnection" connectionString="Data Source=localhost;Initial Catalog=DestinationDatabase;User ID=username;password=password;Integrated Security=False;" providerName="System.Data.SqlClient" />
	</connectionStrings>
	<appSettings>
		<clear />
		<!--
			Sets the directory to save log files. Default value is "\logs"
			under the same directory as the app executed.
		-->
		<add key="LogPath" value="\logs" />

		<!--
			Sets the SSIS package file path. Its file extension is .dtsx.
		-->
		<add key="MsSql.SsisPackagePath" value="C:\SSIS Packages\aliensync.dtsx" />
		<!--
			Sets the executable path where TableDiff is installed.
		-->
		<add key="MsSql.TableDiffExecutablePath" value="C:\Program Files\Microsoft SQL Server\100\COM" />
	</appSettings>
</configuration>